{
  "name": "keymash",
  "version": "0.0.0",
  "description": "A type-safe, performance-oriented keyboard library using bitwise operations for O(1) chord lookup",
  "type": "module",
  "main": "./dist/lib/keymash.js",
  "module": "./dist/lib/keymash.js",
  "types": "./dist/lib/lib/keymash.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib/keymash.js",
      "types": "./dist/lib/lib/keymash.d.ts"
    },
    "./core": {
      "import": "./dist/lib/core.js",
      "types": "./dist/lib/lib/keymash-core.d.ts"
    },
    "./react": {
      "import": "./dist/lib/react.js",
      "types": "./dist/lib/lib/keymash-react.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "keyboard",
    "shortcuts",
    "keybindings",
    "hotkeys",
    "typescript",
    "bitwise",
    "react",
    "hooks"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zetlen/keymash.git"
  },
  "bugs": {
    "url": "https://github.com/zetlen/keymash/issues"
  },
  "homepage": "https://zetlen.github.io/keymash/",
  "scripts": {
    "prepare": "lefthook install",
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:lib:types": "tsc -p tsconfig.lib.json --emitDeclarationOnly",
    "prebuild": "pnpm doc:typedoc",
    "postbuild:lib": "pnpm doc:sizes",
    "prepublishOnly": "pnpm build:lib",
    "preview": "vite preview",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "doc:typedoc": "typedoc",
    "doc:sizes": "node scripts/update-readme-modules.js",
    "doc:build": "pnpm doc:typedoc && pnpm doc:sizes"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@chromatic-com/storybook": "^4.1.3",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "jsdom": "^27.4.0",
    "lefthook": "^2.0.13",
    "playwright": "^1.57.0",
    "react": "^19.2.3",
    "react-code-blocks": "^0.1.6",
    "react-dom": "^19.2.3",
    "storybook": "^10.1.11",
    "terser": "^5.44.1",
    "typedoc": "^0.28.16",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.16"
  }
}
